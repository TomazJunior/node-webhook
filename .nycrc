{
    "check-coverage": true,
    "per-file": true,
    "include": [
        "**/**/*.js"
    ],
    "exclude": [
        "**/**/*.spec.js",
        "**/coverage/",
        "**/lib/request/request.js",
        "**/lib/webhook/webhook.js",
        "**/index.js"
    ],
    "reporter": [
        "lcov",
        "text-summary"
    ],
    "cache": false,
    "all": true,
    "report-dir": "./coverage"
}